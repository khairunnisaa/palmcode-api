                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Booking": {},
            "Country": {},
            "IdVerification": {}
        },
        "securitySchemes": {
